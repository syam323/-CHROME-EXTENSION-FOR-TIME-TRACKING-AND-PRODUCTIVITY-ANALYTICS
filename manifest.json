{
  "manifest_version": 3,
  "name": "Auto Time Analytics",
  "version": "1.0",
  "description": "Automatically track and display productive site analytics daily.",
  "permissions": ["storage", "tabs"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png"
  }
}
